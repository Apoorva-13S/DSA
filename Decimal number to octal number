//WAP to convert decimal number to octal number.

#include<stdio.h>
int main(){
    int num,oct=0,place = 1;
    printf("Enter the number :");
    scanf("%d",&num);
    int temp = num ;
    while(num!=0){
        int rem = num%8;
        oct = oct + rem * place ;
        num = num/8 ;
        place = place * 10;
    }
    printf("Binary of %d is %d",temp , oct);
    return 0;
}
