//WAP to convert decimal number to binary number.

#include<stdio.h>
int main(){
    int num,bin=0,place = 1;
    printf("Enter the number :");
    scanf("%d",&num);
    int temp = num ;
    while(num!=0){
        int rem = num%2;
        bin = bin + rem * place ;
        num = num/2 ;
        place = place * 10;
    }
    printf("Binary of %d is %d",temp , bin);
    return 0;
}
